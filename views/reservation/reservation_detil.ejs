            <h3><i class="fa fa-calendar"></i> Detail Reservasi</h3>
        </div>
        <div class="c10"></div>
        <form>
            <table>
                <tr>
                    <td>Order ID</td>
                    <td>: <%=detilReservation[0].id%></td>
                </tr>
                <tr>
                    <td>Nama</td>
                    <td>: <%=detilReservation[0].Customer.name%></td>
                </tr>
                <tr>
                    <td>Hari</td>
                    <td>: <%=detilReservation[0].Schedule.day%></td>
                </tr>
                <tr>
